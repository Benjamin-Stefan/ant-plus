        "dist/"
    ],
    "directories": {
        "example": "./sample",
        "lib": "./src"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Benjamin-Stefan/ant-plus-next.git"
    },
    "bugs": {
        "url": "https://github.com/Benjamin-Stefan/ant-plus-next/issues"
